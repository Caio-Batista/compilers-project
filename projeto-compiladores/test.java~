import flex.cup;

public class Pessoa {
   private String nome;
   private int idade, cpf;
   
   public int getIdade() {
      return idade;
   }

   public int getCPF() {
      return cpf;
   }

   public void setIdade(int idade) {
      this.idade = idade;
   }
   
   public void setCPF(int cpf) {
      this.cpf = cpf;
   }

   public String getNome() {
      return nome;
   }

   public void setNome(String nome) {
      this.nome = nome;
   }
}

