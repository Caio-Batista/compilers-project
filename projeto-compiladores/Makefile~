JAVA=java
JAVAC=javac
PATH_JFLEX=jflex-1.6.1/bin/jflex
CUPJAR=jflex-1.6.1/lib/java-cup-11a.jar
CUP=$(JAVA) -jar $(CUPJAR) <
CP=.:$(CUPJAR)

run: 
	$(JFLEX) java_flex.flex
	java -jar $(CUPJAR) -interface -parser JavaParser java.cup
	javac -classpath $(CUPJAR):. Main.java
	java -classpath $(CUPJAR):. Main test.java

clean:
	rm -f Lexer.java JavaParser.java sym.java *.class *~
